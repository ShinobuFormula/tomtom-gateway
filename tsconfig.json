{
	"compilerOptions": {
		"lib": ["ESNext", "DOM"],
		"target": "ESNext",
		"declaration": false,
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	},
	"include": ["**/*"],
	"exclude": ["node_modules", "**/*.spec.ts"],
}
